regexp="(?P<date>\S+)\s+(?P<sensor>\S+)\s+(?P<source>\S+)\s+Alert\s+Level:\s([4-9][0-9]*)\s*;\s+Rule:\s+(?P<rule_id>\d+)\s+-\s+(?P<rules_comment>[^;]*);\s+Location:\s+\S*\s*(?P<hostname>\S+)->(?P<filelog>\S+);\s+srcip:\s+(?P<ipsrc>\S+);(?P<event>.*$)"
